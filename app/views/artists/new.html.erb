<h1>Artist Registration Page</h1>
<%= form_with url: artists_register_path, method: :post, local: true do |f| %>
   <%= f.label :name, "Name:" %>
   <%= f.text_field :name %>
   <%= f.label :location, "Location:" %>
   <%= f.text_field :location %>
   <%= f.label :phone, "Phone:" %>
   <%= f.telephone_field :phone %>
   <%= f.label :genre, "Genre:" %>
   <%= f.text_field :genre %>
   <%= f.label :bio, "Tell me about yourself:" %>
   <%= f.text_field :bio %>
   <%= f.label :role %>
   <%= f.hidden_field :role, "#{params[:role]}" %>
   <%= f.submit %>
   <% end %>